{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/react"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "style": "styled-components",
        "linter": "eslint",
        "babel": true
      },
      "component": {
        "style": "styled-components"
      },
      "library": {
        "style": "styled-components",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "court-a[[",
  "projects": {
    "court-a[[": {
      "root": "apps/court-a[[",
      "sourceRoot": "apps/court-a[[/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/web:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/court-a[[",
            "index": "apps/court-a[[/src/index.html",
            "main": "apps/court-a[[/src/main.tsx",
            "polyfills": "apps/court-a[[/src/polyfills.ts",
            "tsConfig": "apps/court-a[[/tsconfig.app.json",
            "assets": [
              "apps/court-a[[/src/favicon.ico",
              "apps/court-a[[/src/assets"
            ],
            "styles": [],
            "scripts": [],
            "webpackConfig": "@nrwl/react/plugins/webpack"
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/court-a[[/src/environments/environment.ts",
                  "with": "apps/court-a[[/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/web:dev-server",
          "options": {
            "buildTarget": "court-a[[:build"
          },
          "configurations": {
            "production": {
              "buildTarget": "court-a[[:build:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/court-a[[/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/court-a[["],
          "options": {
            "jestConfig": "apps/court-a[[/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "court-a[[-e2e": {
      "root": "apps/court-a[[-e2e",
      "sourceRoot": "apps/court-a[[-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/court-a[[-e2e/cypress.json",
            "tsConfig": "apps/court-a[[-e2e/tsconfig.e2e.json",
            "devServerTarget": "court-a[[:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "court-a[[:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/court-a[[-e2e/**/*.{js,ts}"]
          }
        }
      }
    }
  }
}
